#include <stdio.h>
#include <stdlib.h>
void swap(int, int);
void swap1(int *, int *);
int main()
{
    int a = 55, b = 66;
    swap(a,b);
    printf("In main: a = %d, b = %d\n", a, b);
    
    swap1(&a, &b);
    
    return 0;
}
void swap(int a, int b)
{
    int temp = a;
    a = b;
    b = temp;
    printf("In swap: a = %d, b = %d\n", a, b);
}
void swap1(int *p, int *t)
{
    int temp;
    printf("&p = %#p\t &t = %#p\n", &p, &t);
    printf("p = %#p\t t = %#p\n", p, t);
    temp = *p;
    *p = *t;
    *t = temp;
    printf("交换数据为：a = %d, b = %d\n", *p, *t);
}
